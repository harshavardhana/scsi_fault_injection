probe begin
{
      dev_major = $1
      dev_minor_min = $2
      dev_minor_max = $3
      inode_lba_flag = $4
      inode_lba_val = $5
      error_type = 1
      access_type = 3
}

probe end
{
      printf("\nDONE\n")
}

